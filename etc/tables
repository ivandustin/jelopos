customer id INTEGER PRIMARYKEY
customer name TEXT NOTNULLUNIQUE
customer address TEXT 
customer contact TEXT 
sale id INTEGER PRIMARYKEY
sale created_at INTEGER NOTNULLDEFAULT(strftime('%s','now'))
sale customer_id INTEGER NOTNULL
sale job_order INTEGER UNIQUE
sale weight REAL 
sale username TEXT NOTNULL
sale note TEXT 
sale terms TEXT 
sale void INTEGER NOTNULLDEFAULT0
item id INTEGER PRIMARYKEY
item created_at INTEGER NOTNULLDEFAULT(strftime('%s','now'))
item name TEXT NOTNULL
item size TEXT NOTNULL
item unit TEXT NOTNULL
item qty INTEGER NOTNULL
item weight REAL NOTNULL
coil_design id INTEGER PRIMARYKEY
coil_design design TEXT NOTNULL
coil id INTEGER PRIMARYKEY
coil created_at INTEGER NOTNULLDEFAULT(strftime('%s','now'))
coil color TEXT NOTNULL
coil thickness REAL NOTNULL
coil width REAL NOTNULL
coil length REAL NOTNULL
coil weight REAL NOTNULL
coil serial TEXT NOTNULL
coil code TEXT NOTNULL
