precut_coil created_at INTEGER NOTNULLDEFAULT(strftime('%s','now'))
precut_coil design TEXT NOTNULL
precut_coil color TEXT NOTNULL
precut_coil thickness REAL NOTNULL
precut_coil width REAL NOTNULL
precut_coil length REAL NOTNULL
precut_coil code TEXT NOTNULL
precut_coil qty INTEGER NOTNULL
sale_item id INTEGER PRIMARYKEY
sale_item created_at INTEGER NOTNULLDEFAULT(strftime('%s','now'))
sale_item sale_id INTEGER NOTNULL
sale_item name TEXT NOTNULL
sale_item size TEXT NOTNULL
sale_item qty INTEGER NOTNULL
sale_item price REAL NOTNULL
sale_item item_id INTEGER 
sale_item void INTEGER NOTNULLDEFAULT0
